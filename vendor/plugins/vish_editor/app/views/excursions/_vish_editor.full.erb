		<!-- Help in menu -->
		<ol id="menubar_help" class="joyRideTipContent">
		   <li data-id="thumbcontent" data-text="Next" i18n-key="i.Next" class="tut_welcome_exc tut_without_arrow Next" data-options="tipLocation:none">
