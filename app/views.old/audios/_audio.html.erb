<%= div_for audio, :class => 'row-fluid' do %>

  <% if audio.file_processing %>
    <%= render :partial => 'audios/audio_processing', :locals => {:audio => audio}%>
  <% else %>
    <%= render :partial => 'audios/audio_processed', :locals => {:audio => audio}%>  
  <% end %>

<% end %>

<div class="clearfloat"></div>
