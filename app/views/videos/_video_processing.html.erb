<ul class="thumbnails">
  <li class="span3">
    <%= link_to image_tag("icons/link_big_gray.png"),video %>
  </li>
  <li class="span9">
    <div class="title">
      <h4 class="bold"><%= link_to truncate_name(video.title, :length => 35), video %></h4>
    </div>
    <p class="processing_text"><%= t('video.processing')%></p>
    <p><%= link_to t('details.msg'), video, :class => "attachment_text_link" %></p>
  </li>
</ul>

