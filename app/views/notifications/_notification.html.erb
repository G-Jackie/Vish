<tr>
  <td>
    <%= div_for(notification) do %>
      <%= render :partial => "notifications/activities/#{ notification.notified_object.verb }",
                 :locals => {:activity => notification.notified_object} %>
      , <%= t('time.ago', :time => time_ago_in_words(notification.created_at)) %>
    <% end %>
  </td>
</tr>