<%=render :partial=> "catalogue/tags"%>
<div id="excursions_catalogue" >
	<%@all_categories.each do |name, the_array| %>
		<h2><%=name%></h2>
		<button class="show_more_catalogue"><%=t('actions.show_more')%></button>
		<ul>
			<%for excursion in the_array%>
				<%=render :partial=> "excursions/excursion", :locals => { :excursion => excursion } %>
			<%end%>
		</ul>
	<%end%>
</div>
