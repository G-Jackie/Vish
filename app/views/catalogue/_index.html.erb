<div class="catalogue">
  <header class="header_catalogue">
    <%show_title ||= false %>
    <%is_home ||= false%>
    <%if show_title%>
      <h1><i class="fa fa-tags"></i><%=t('catalogue.title')%></h1>
      <hr>
    <%end%>
    <%= render :partial => "catalogue/tags", :locals =>{ :is_home=> is_home}%>
  </header>

  <%= render :partial => "catalogue/main", :locals =>{ :is_home=> is_home}%>
</div>