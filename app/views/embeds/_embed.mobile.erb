<%= div_for embed, :class => 'row-fluid' do %>
  <%= javascript_tag do %>
    $('#embed_<%= embed.id.to_s %>').parents('.thumbnails').children().first().html('<%= j link_to raw("<i class=\"icon_repository75-75_embed\"></i>"), embed, :class => "" %>');
  <% end %>

  <ul class="thumbnails">
    <li class="span9">
      <div class="title">
        <h4 class="bold ellipsis w190px">
          <%= link_to embed.title, embed %>
        </h4>
      </div>
    </li>
  </ul>
<% end %>

<div class="clearfloat"></div>
