<div class="model_with_details span3 box-result">
  <ul class="thumbnails">
    <li class="span1 margin-clear">
      <%= link_to  "", embed, :class => icon75_class_for(embed) , :alt => embed.title %>
    </li>
    <li class="span">
      <div class="caption">
        <h4><span class="size16"><%= link_to truncate_name(embed.title, :length => 40), embed, :class => "size14" %></span></h4>
        <h5><span class="green"><%=embed.visit_count.to_s%></span> <%= t('follow.views') %></h5>
      </div>
    </li>
  </ul>
</div>
