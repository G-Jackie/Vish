<%= div_for excursion, :class => 'row-fluid' do %>
  <%= javascript_tag do %>
    $('#excursion_<%= excursion.id.to_s %>').parents('.thumbnails').children().first().html('<%= j link_to excursion_thumb_for(excursion, 130), excursion, :class => "container" %>');
  <% end %>
  <ul class="thumbnails">
    <li class="span9">
        <div class="title">
          <h4 class="bold ellipsis w190px"><%= link_to excursion.title, excursion %></h4>
        </div>
    </li>
  </ul>
<% end %>

