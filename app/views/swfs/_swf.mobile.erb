<%= div_for swf, :class => 'row-fluid' do %>
  <%= javascript_tag do %>
    $('#swf_<%= swf.id.to_s %>').parents('.thumbnails').children().first().html('<%= j link_to raw("<i class=\"icon75-swf\">"), download_document_path(swf), :class => "" %>');
  <% end %>
  <ul class="thumbnails">
    <li class="span9">
      <div class="title">
        <h4 class="bold ellipsis w190px">
          <%= link_to swf.title, download_document_path(swf) %>
        </h4>
      </div>
    </li>
  </ul>
<% end %>

