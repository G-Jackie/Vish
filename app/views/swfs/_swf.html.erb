<%= div_for swf, :class => 'row-fluid' do %>
	<ul class="thumbnails">
    <li class="span3">
		  <iframe src="<%= swf.file %>" width="75" height="75">...</iframe>
    </li>
    <li class="span9">
      <div class="title">
        <h4><%= truncate_name(swf.title, :length => 35) %></h4>
      </div>

      <p><%= simple_format auto_link(swf.description) %></p>
      <p><%= link_to t('details.msg'), swf, :class => "attachment_text_link" %></p>
    </li>
	</ul>
<% end %>

<div class="clearfloat"></div>
