<ul class="items entities_front">
	<% popular_entities(20,home_models).each do |resource| %>
		<%= render :partial => 'entities/entity', :locals => { :entity => resource, :prefix_id => "frontpage" } %>
	<% end %>
</ul>

<script>
	$("div[bs-img]").each(function() {
		$(this).backstretch( $(this).attr("bs-img") );
	});
</script>

