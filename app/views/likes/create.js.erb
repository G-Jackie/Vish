if ($(".verb_like.like_<%= dom_id(@like.object) %>").length <1) {
  $(".like_<%= dom_id(@like.object) %>").html("<%= escape_javascript(render partial: 'activities/like', locals: { activity: @like.object }) %>");
} else {
  $(".verb_like.like_<%= dom_id(@like.object) %>").data('method', 'delete');
  $(".verb_like.like_<%= dom_id(@like.object) %> .fa-star-o").removeClass("fa-star-o").addClass("fa-star");
  var new_num = parseInt($(".verb_like.like_<%= dom_id(@like.object) %>").closest(".like").children(".count").value()) + 1;
   $(".verb_like.like_<%= dom_id(@like.object)").closest(".like").children(".count").html(new_num);
  
}
