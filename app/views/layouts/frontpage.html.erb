<!-- HTML5 DOCTYPE -->
<!DOCTYPE html>
<html lang="<%= I18n.locale.to_s %>">
<%= render :partial => 'layouts/head' %>
<body>
<div class="container">
  <header id="headerFront" class="row span7 offset5">
    <%= render :partial => "layouts/header_frontpage" %>
  </header>
  <section id="main" class="row span12">
    <div class="space_double_center"></div>
    <div id="flash">
      <%= render :partial => "layouts/flash", :locals => {:flash => flash} %>
    </div>
    <%= yield %>
    <div class="space_center"></div>
  </section>
</div>
<section id="top" class="white">
  <%#= render :partial => "excursions/top10" %>
</section>
<div class="container">
  <div class="space_double_center"></div>
  <footer class="row span12">
    <%= render :partial => "layouts/footer" %>
  </footer>
</div>
</body>
</html>
