<% case params[:active_tab] %>
<%  when 'net' then %>
  $('#net_load_more_button').remove();
  $('#wall').append("<%= escape_javascript render(:partial => 'activities/wall', :locals => { :activities => current_subject.wall(:home).page(params[:page_net]), :is_active_wall => true }) %>");
<%  when 'me' then %>
  $('#me_load_more_button').remove();
  $('#wall-me').append("<%= escape_javascript render(:partial => 'activities/wall_me', :locals => { :activities => current_subject.wall(:profile, :for => current_user).page(params[:page_me]), :is_active_wall => true }) %>");
<% end %>

