
<!-- corner -->
<div class="corner-item">
  <div class="corner-item2">
    <div class="corner-color"></div>
    <i class="icon-scorm-new"></i>
  </div>
  <div class="corner-background"></div>
</div>

<h3 class="title_WS resource_title_WS"><%=t('workshop.activities.resource.title')%> <span class="blue"> OfficeDoc</span></h3>
<button href="#" id="showMore" status="showMore" class="ui-state-default ui-corner-all"><a><%=t('excursion.actions.show_more')%></a></button>

<%= div_for officedoc, :class => 'object_WS' do %>

  <div class="officedoc_show">
    <iframe src="http://docs.google.com/viewer?url=<%= Vish::Application.config.full_domain + officedoc.file.url %>&embedded=true" width="100%" height="500" style="border: none;"></iframe>
  </div>

<% end %>

<div class="resource_ws_info">

  <%= render partial: "common_documents/info_workshop", locals: { document: scormfile } %>
  <div class="actions">
    <button href="#" class="ui-state-default ui-corner-all"><a><%=t('excursion.actions.download')%></a></button>
    <button href="#" class="ui-state-default ui-corner-all"><a href="<%=officedoc_path(officedoc, :format => :full)%>" target="_blank"><i class="icon-fullscreen"></i><%= t("officedoc.view_full") %></a></button>
</div>
</div>