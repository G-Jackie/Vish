<div class="space_center"></div>
<div class="header_text">
  <%= t('resource.title.other') %>
</div>
<div class="menu_resources">
  <a class="action" href="/common_documents/index">
    <%=link_to(t('go.repository'),[subject, Document.new])%>
  </a>
</div>
<div class=" row space_center"></div>
<% for r in subject_resources(subject, :scope => :me) %>
  <% if r.is_a? Embed %>
    <%= render :partial => "embeds/profile", :locals => { :embed => r, :div_class => 'row' } %>
  <% else %>
    <%= render :partial => "common_documents/profile", :locals => { :document => r, :div_class => 'row' } %>
  <% end %>
<% end %>




<div class="row space_center"></div>
