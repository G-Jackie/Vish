<li class="helpheader helpbutton">   
<i class="icon-question-sign helpEvent helppopover" id="helpeventid" data-placement='left' aria-hidden="true" data-original-title="<%= t('document.help.event_html')%>" href='#' value="" ></i>
<div class="contenthelpEvent" id="contenthelpeventid" style="display: none">
  <div>
          <%= t('document.help.event1_html')%>  <br/>
          <%= t('document.help.event2_html')%>  <br/>
          <%= t('document.help.event3_html')%>  <br/>
          <%= t('document.help.event4_html')%>  <br/>
          <%= t('document.help.event5_html')%>
  </div>
  <div class="modal-footer popoverfix"><%= t('document.help.overview_html')%></div>
</div>
</li>
<%content_for :javascript do%>
  $('#helpeventid').popover({ 
    html : true,
    content: function() {
      return $('#contenthelpeventid').html();
    }
  });
<%end%>
